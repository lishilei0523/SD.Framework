<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
    <Version>2.6.2</Version>
    <Description>SD.Framework 基础设施 - 仓储基础 - EntityFrameworkCore实现</Description>
  </PropertyGroup>

  <ItemGroup Condition="$(TargetFramework.Equals('net8.0'))">
    <PackageReference Include="Microsoft.EntityFrameworkCore.Proxies" Version="8.0.22" />
  </ItemGroup>

  <ItemGroup Condition="$(TargetFramework.Equals('net9.0'))">
    <PackageReference Include="Microsoft.EntityFrameworkCore.Proxies" Version="9.0.11" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SD.Toolkits.EntityFrameworkCore" Version="2.3.2" />
    <PackageReference Include="SD.IOC.Core" Version="2.3.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SD.Infrastructure\SD.Infrastructure.csproj" />
  </ItemGroup>
</Project>
