<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--应用程序配置-->
  <appSettings>
    <!--工作流持久化连接字符串键-->
    <add key="WorkflowPersistenceConnection" value="DefaultConnection" />
    <!--工作流持久化模式-->
    <add key="WorkflowPersistenceMode" value="Temporary" />
    <!--最大实例锁定重试次数-->
    <add key="MaxInstanceLockedRetriesCount" value="20" />
  </appSettings>
</configuration>