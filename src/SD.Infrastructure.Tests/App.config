    <!--定时任务所在程序集-->
    <crontabAssembly>定时任务所在程序集</crontabAssembly>
    <!--工作流所在程序集-->
    <workflowAssembly>工作流所在程序集</workflowAssembly>
    <!--数据表名前缀-->
    <tablePrefix>数据表名前缀</tablePrefix>
    <!--身份过期时间，单位：分钟，默认值：20-->
    <authenticationTimeout>20</authenticationTimeout>
  </sd.framework>
