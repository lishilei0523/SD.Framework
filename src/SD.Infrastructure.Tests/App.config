<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--SD.Framework配置节点-->
    <section name="sd.framework" type="SD.Infrastructure.FrameworkSection, SD.Infrastructure" />
  </configSections>

  <!--连接字符串-->
  <connectionStrings>
    <!--默认连接字符串-->
    <add name="DefaultConnection" connectionString="Data Source=.;Initial Catalog=Realgoal.MOM.v1.0;User Id=sa;Password=realgoal123!;MultipleActiveResultSets=true;" providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <!--SD.Framework配置-->
  <sd.framework>
    <!--服务名称-->
    <serviceName>服务名称</serviceName>
    <!--服务显示名称-->
    <serviceDisplayName>服务显示名称</serviceDisplayName>
    <!--服务描述-->
    <serviceDescription>服务描述</serviceDescription>
    <!--实体所在程序集-->
    <entityAssembly>实体所在程序集</entityAssembly>
    <!--实体配置所在程序集-->
    <entityConfigAssembly>实体配置所在程序集</entityConfigAssembly>
    <!--领域事件源所在程序集-->
