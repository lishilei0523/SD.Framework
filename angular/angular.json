{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "sd-infrastructure-values": {
            "projectType": "library",
            "root": "projects/sd-infrastructure-values",
            "sourceRoot": "projects/sd-infrastructure-values/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/sd-infrastructure-values/tsconfig.lib.json",
                        "project": "projects/sd-infrastructure-values/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/sd-infrastructure-values/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/sd-infrastructure-values/src/test.ts",
                        "tsConfig": "projects/sd-infrastructure-values/tsconfig.spec.json",
                        "karmaConfig": "projects/sd-infrastructure-values/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/sd-infrastructure-values/tsconfig.lib.json",
                            "projects/sd-infrastructure-values/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "sd-infrastructure-angular": {
            "projectType": "library",
            "root": "projects/sd-infrastructure-angular",
            "sourceRoot": "projects/sd-infrastructure-angular/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/sd-infrastructure-angular/tsconfig.lib.json",
                        "project": "projects/sd-infrastructure-angular/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/sd-infrastructure-angular/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/sd-infrastructure-angular/src/test.ts",
                        "tsConfig": "projects/sd-infrastructure-angular/tsconfig.spec.json",
                        "karmaConfig": "projects/sd-infrastructure-angular/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/sd-infrastructure-angular/tsconfig.lib.json",
                            "projects/sd-infrastructure-angular/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "sd-infrastructure-values"
}
